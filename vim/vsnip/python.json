    "body": ["import dask.bag as db"]
  },
  "import-numpy": {
    "prefix": ["np"],
    "body": ["import numpy as np"]
  },
  "import-pandas": {
    "prefix": ["pd"],
    "body": ["import pandas as pd"]
  },
  "import-matplotlib": {
    "prefix": ["plt"],
    "body": ["import matplotlib.pyplot as plt"]
  },
  "breakpoint": {
    "prefix": ["bp"],
    "body": ["breakpoint()"]
  },
